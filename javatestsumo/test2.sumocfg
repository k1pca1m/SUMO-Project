<?xml version="1.0" encoding="UTF-8"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">

    <input>
        <!-- Tera improved network aur routes -->
        <net-file value="test2.net.xml"/>
        <route-files value="test2.rou.xml"/>
    </input>

    <time>
        <!-- Simulation 0 se 3600 second (1 hour) tak chalegi -->
        <begin value="0"/>
        <end value="3600"/>
        <!-- default step-length 1s hai; chahe to yahan explicitly rakhe -->
        <step-length value="1.0"/>
    </time>

    <report>
        <!-- Console pe warnings/debug kam rakhne ke liye -->
        <verbose value="false"/>
        <no-step-log value="true"/>
    </report>

</configuration>
